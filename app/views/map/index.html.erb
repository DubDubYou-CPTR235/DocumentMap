<% content_for :head do %>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyApJ5eD8YdSK5SymAX8wp-_lx2luWItJQE"></script>
  <script src="http://google-maps-utility-library-v3.googlecode.com/svn/tags/markerclusterer/1.0.2/src/markerclusterer_compiled.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <title>DocumentMap</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
<% end %>

<h3>MAP</h3>

<script>
var json = <%= @documents %>;
google.maps.event.addDomListener(window, 'load', initialize);
</script>

<div id="map-container">
  <div id="map-canvas"></div>
</div>

<ul id="results">
</ul>
